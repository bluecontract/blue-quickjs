{
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "name": "quickjs-native-harness",
  "projectType": "application",
  "root": "tools/quickjs-native-harness",
  "sourceRoot": "tools/quickjs-native-harness/src",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "cache": true,
      "inputs": ["default", "quickjsSubmodule"],
      "outputs": ["{workspaceRoot}/tools/quickjs-native-harness/dist"],
      "options": {
        "command": "bash tools/quickjs-native-harness/scripts/build.sh"
      }
    },
    "test": {
      "executor": "nx:run-commands",
      "dependsOn": ["build"],
      "options": {
        "command": "bash tools/quickjs-native-harness/scripts/test.sh"
      }
    }
  },
  "tags": []
}
